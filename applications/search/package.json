{
  "name": "search",
  "version": "0.2.4-SNAPSHOT",
  "description": "Search",
  "scripts": {
    "precommit": "lint-staged",
    "start": "node start.js",
    "dev": "webpack-dev-server --config webpack.dev.config.js",
    "build": "webpack --config webpack.prod.config.js -p",
    "server": "export NODE_ENV=production && node start.js",
    "test_jest": "jest --config=jest.config.json",
    "test": "jest --config=./jest.config.json --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "tdd": "npm test -- --watch",
    "lint": "eslint ./ --ext=.js,.jsx --fix"
  },
  "lint-staged": {
    "gitDir": "../../",
    "linters": {
      "*.{js,jsx}": [
        "lint",
        "git add"
      ]
    }
  },
  "license": "ISC",
  "dependencies": {
    
  },
  "devDependencies": {

  }
}
